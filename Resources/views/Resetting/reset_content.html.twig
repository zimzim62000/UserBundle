{% trans_default_domain 'FOSUserBundle' %}

<div class="row panel">
    <div class="small-12 columns text-center">

        <form action="{{ path('fos_user_resetting_reset', {'token': token}) }}" {{ form_enctype(form) }} method="POST"
              class="fos_user_resetting_reset">
            {{ form_widget(form) }}
            <div>
                <input type="submit" class="tiny button success" value="{{ 'resetting.reset.submit'|trans }}"/>
            </div>
        </form>

    </div>
</div>
